{
    "env": {
      "browser": true,
      "es2020": true
    },
    "extends": "trybe-frontend",
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": ["react", "sonarjs"]
  }

